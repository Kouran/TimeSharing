<h1>Listing utentes</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cognome</th>
      <th>Nickname</th>
      <th>Data nascita</th>
      <th>Citta residenza</th>
      <th>Professione</th>
      <th>Email</th>
      <th>Telefono</th>
      <th>Competenze</th>
      <th>Portafoglio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @utentes.each do |utente| %>
      <tr>
        <td><%= utente.nome %></td>
        <td><%= utente.cognome %></td>
        <td><%= utente.nickname %></td>
        <td><%= utente.data_nascita %></td>
        <td><%= utente.citta_residenza %></td>
        <td><%= utente.professione %></td>
        <td><%= utente.email %></td>
        <td><%= utente.telefono %></td>
        <td><%= utente.competenze %></td>
        <td><%= utente.portafoglio %></td>
        <td><%= link_to 'Show', utente %></td>
        <td><%= link_to 'Edit', edit_utente_path(utente) %></td>
        <td><%= link_to 'Destroy', utente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Utente', new_utente_path %>
